
record(waveform, "$(D):Mod-Warn"){
    field(PINI, "YES")
    field(SCAN, "")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getModWarnTree $(PORT) $(A)")
    field(FTVL, "DOUBLE")
    field(NELM, "33")
}

record(longin, "$(D):Mod-StdWarnGroup-Mon"){
    field(INP,  "$(D):Mod-Warn[0] CP MSS")
    field(DESC, "Std warn group")
}

