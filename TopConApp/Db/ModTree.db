
record(waveform, "$(D):ModTree"){
    field(SCAN, "30 second")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getArray(getModTree) $(P)")
    field(FTVL, "DOUBLE")
    field(NELM, "66")
    field(PRIO, "LOW")
}

record(longin, "$(D):ModErrGroup-Mon"){
    field(SCAN, "Passive")
    field(INP,  "$(D):ModTree.VAL[0] CP MSS")
    field(DESC, "Mod + Ext error group")
    field(DTYP, "Soft Channel")
    field(EGU, "")
}

