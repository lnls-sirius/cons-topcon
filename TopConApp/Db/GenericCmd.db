
record(bo, "$(D):Connect-Cmd"){
    field(DESC, "Attempt to connect to the device")
    field(ONAM, "")
    field(ZNAM, "")
    field(DISA, "1")
    field(DISV, "0")
    field(PRIO, "HIGH")
    field(DISS, "NO_ALARM")
    field(FLNK, "")
    field(HIGH, "0.1")
    field(DISV, "0")
    field(SDIS, "$(D):Connect-Cmd")

    field(DTYP, "stream")
    field(OUT,  "@Regatron.proto cmd(cmdConnect) $(P)")
}

