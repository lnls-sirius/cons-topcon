
record(waveform, "$(D):Sys-Tree"){
    field(SCAN, "30 second")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getArray(getSysTree) $(P)")
    field(FTVL, "DOUBLE")
    field(NELM, "66")
    field(PRIO, "LOW")
}

record(longin, "$(D):Sys-ErrGroup-Mon"){
    field(SCAN, "Passive")
    field(INP,  "$(D):Sys-Tree.VAL[0] CP MSS")
    field(DESC, "Sys  +  error group")
    field(DTYP, "Soft Channel")
    field(EGU, "")
}

