
record(bo, "$(D):Save-Cmd"){
    field(DESC, "Save settings to non-volatile memory")
    field(ONAM, "")
    field(ZNAM, "")
    field(DISA, "1")
    field(DISV, "0")
    field(PRIO, "HIGH")
    field(DISS, "NO_ALARM")
    field(FLNK, "")
    field(HIGH, "0.1")
    field(DISV, "0")
    field(SDIS, "$(D):Save-Cmd")

    field(DTYP, "stream")
    field(OUT,  "@Regatron.proto cmd(cmdStoreParam) $(P)")
}

record(bo, "$(D):Reset-Cmd"){
    field(DESC, "Clear Errors and/or warnings")
    field(ONAM, "")
    field(ZNAM, "")
    field(DISA, "1")
    field(DISV, "0")
    field(PRIO, "HIGH")
    field(DISS, "NO_ALARM")
    field(FLNK, "")
    field(HIGH, "0.1")
    field(DISV, "0")
    field(SDIS, "$(D):Reset-Cmd")

    field(DTYP, "stream")
    field(OUT,  "@Regatron.proto cmd(cmdClearErrors) $(P)")
}
