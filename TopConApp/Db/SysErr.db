
record(waveform, "$(D):Sys-Err"){
    field(PINI, "YES")
    field(SCAN, "")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getSysErrTree $(PORT) $(A)")
    field(FTVL, "DOUBLE")
    field(NELM, "33")
}

record(longin, "$(D):Sys-StdErrGroup-Mon"){
    field(INP,  "$(D):Sys-Err[0] CP MSS")
    field(DESC, "Std warn group")
}

