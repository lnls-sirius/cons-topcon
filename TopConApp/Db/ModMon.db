
record(waveform, "$(D):Mod-MinMaxNom-Mon"){
    field(SCAN, "30 second")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getArray(getModMinMaxNom) $(P)")
    field(FTVL, "DOUBLE")
    field(NELM, "12")
    field(PRIO, "LOW")
}

