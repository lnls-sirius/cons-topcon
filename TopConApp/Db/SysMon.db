
record(waveform, "$(D):Sys-MinMaxNom-Mon"){
    field(SCAN, "30 second")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getArray(getSysMinMaxNom) $(P)")
    field(FTVL, "DOUBLE")
    field(NELM, "12")
    field(PRIO, "LOW")
}

