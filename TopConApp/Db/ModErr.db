
record(waveform, "$(D):Mod-Err"){
    field(PINI, "YES")
    field(SCAN, "")
    field(DTYP, "stream")
    field(INP,  "@Regatron.proto getModErrTree $(PORT) $(A)")
    field(FTVL, "DOUBLE")
    field(NELM, "33")
}

record(longin, "$(D):Mod-StdErrGroup-Mon"){
    field(INP,  "$(D):Mod-Err[0] CP MSS")
    field(DESC, "Std warn group")
}

